  <!-- Powered by Terminus.io -->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>余人自娱-商家中心首页</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/assets/styles/vendor.css">
    <link rel="stylesheet" href="/assets/styles/app.css">
  </head>
  <body class="user-background">
    <div class="ceiling-outer"></div>
    <div class="seller-sidebar"></div>
    <div class="user-container">
      <form class="form-horizontal user-form" role="form" id="seller-profile-form">
        <h2>商家资料</h2>
        <br>
        <div class="form-group">
          <label for="inputEmail3" class="col-sm-4 control-label">店铺名</label>
          <div class="col-sm-8">
            <input type="hidden" name="id">
            <input type="text" class="form-control" name="shopName" id="user-name" placeholder="" disabled>
          </div>
        </div>
        <div class="form-group">
          <label for="inputPassword3" class="col-sm-4 control-label">固定电话</label>
          <div class="col-sm-8">
            <input type="text" class="form-control" name="telephone" id="real-telephone" placeholder="">
          </div>
        </div>
        <div class="form-group">
          <label for="inputPassword3" class="col-sm-4 control-label">店铺手机</label>
          <div class="col-sm-8">
            <input type="text" class="form-control" name="phone" id="user-phone" placeholder="输入正确手机号">
          </div>
        </div>
        <div class="form-group">
          <label for="inputPassword3" class="col-sm-4 control-label">地址</label>
          <div class="col-sm-8">
            <input type="text" class="form-control" name="address" id="user-address" placeholder="输入正确地址">
          </div>
        </div>
        <div class="form-group">
          <label for="inputPassword3" class="col-sm-4 control-label">店铺LOGO</label>
          <div class="col-sm-8">
            <img src="" alt="" id="image-url" width="100" height="100">
            <input type="hidden" name="exFileName" />
            <input type="hidden" class="form-control" name="imageUrl">
            <button class="file-input-wrapper btn btn-warning">
              <span>上传</span><input type="file" name="file" style="left: -196.5px; top: 10px;" multiple>
            </button>
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-offset-2 col-sm-8">
            <button type="submit" class="btn btn-default btn-lg btn-primary">修改</button>
          </div>
        </div>
      </form>
    </div>
